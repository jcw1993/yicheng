<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yicheng.pojo">
    <class name="OrderCloth" table="order_cloth">
        <id name="id" column="id">
            <generator class="increment" />
        </id>
        
        <property name="clothId" type="integer" column="cloth_id" />
        <property name="orderNumber" type="string" column="order_number" />
        <property name="deliveryDate" type="timestamp" column="delivery_date" />
        <property name="count" type="integer" column="count" />
    </class>

</hibernate-mapping>
