<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.yicheng.pojo">
    <class name="ClothSize" table="cloth_size">
        <id name="id" column="id">
            <generator class="increment" />
        </id>
        <property name="clothId" type="integer" column="cloth_id" />
		<property name="clothColorId" type="integer" column="cloth_color_id" />
        <property name="sizeType" type="integer" column="size_type" />
        <property name="count" type="integer" column="count" />
    </class>

</hibernate-mapping>
